{
   "name": "next-clean",
   "author": "BinarySenseiii",
   "description": "Open-source starter template for building Next.js applications with Shadcn and Tailwind CSS.",
   "keywords": [
      "Next.js",
      "TypeScript",
      "React",
      "Tailwind CSS",
      "Shadcn",
      "starter template",
      "web app",
      "open-source"
   ],
   "version": "0.1.1",
   "private": true,
   "type": "module",
   "scripts": {
      "analyze": "npx cross-env ANALYZE=true yarn build",
      "build": "next build",
      "dev": "next dev --turbopack",
      "typecheck": "tsc --noEmit",
      "start": "next start",
      "upgrade:latest": "yarn upgrade-interactive --latest",
      "lint": "next lint",
      "lint:fix": "next lint --fix",
      "format:write": "prettier --write \"**/*.{ts,js,jsx,tsx,mdx}\"",
      "clean": "rm -rf .next",
      "clean-build": "yarn run clean && yarn run build",
      "reinstall": "yarn run clean && rm -rf node_modules && rm yarn.lock && yarn install",
      "prepare": "husky || true",
      "commitlint": "commitlint --edit",
      "check-unused:pkgs": "depcheck"
   },
   "lint-staged": {
      "./src/**/*.{ts,js,jsx,tsx}": [
         "eslint --ignore-pattern .gitignore --fix",
         "prettier --ignore-path .gitignore --write"
      ]
   },
   "dependencies": {
      "@hookform/resolvers": "^4.1.3",
      "@t3-oss/env-nextjs": "^0.12.0",
      "@tanstack/react-query": "^5.69.0",
      "@tanstack/react-query-devtools": "^5.69.0",
      "axios": "^1.8.4",
      "clsx": "^2.1.1",
      "immer": "^10.1.1",
      "jiti": "^2.4.2",
      "lodash": "^4.17.21",
      "lucide-react": "^0.484.0",
      "motion": "^12.6.2",
      "next": "15.2.4",
      "nuqs": "^2.4.1",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "react-hook-form": "^7.54.2",
      "schema-dts": "^1.1.5",
      "sonner": "^2.0.1",
      "tailwind-merge": "^3.0.2",
      "ts-pattern": "^5.6.2",
      "uuid": "^11.1.0",
      "zod": "^3.24.2",
      "zustand": "^5.0.3"
   },
   "devDependencies": {
      "@commitlint/cli": "^19.8.0",
      "@commitlint/config-conventional": "^19.8.0",
      "@eslint/js": "^9.23.0",
      "@next/bundle-analyzer": "^15.2.4",
      "@next/eslint-plugin-next": "^15.2.4",
      "@tailwindcss/postcss": "^4.0.17",
      "@tanstack/eslint-plugin-query": "^5.68.0",
      "@types/eslint": "^9.6.1",
      "@types/lodash": "^4.17.16",
      "@types/node": "^22.13.13",
      "@types/react": "^19.0.12",
      "@types/react-dom": "^19.0.4",
      "@typescript-eslint/parser": "^8.28.0",
      "eslint": "^9.23.0",
      "eslint-config-next": "15.2.4",
      "eslint-config-prettier": "^10.1.1",
      "eslint-plugin-import": "^2.31.0",
      "eslint-plugin-promise": "^7.2.1",
      "eslint-plugin-react": "^7.37.4",
      "eslint-plugin-simple-import-sort": "^12.1.1",
      "globals": "^16.0.0",
      "husky": "^9.1.7",
      "lint-staged": "^15.5.0",
      "postcss": "^8.5.3",
      "prettier": "^3.5.3",
      "prettier-plugin-tailwindcss": "^0.6.11",
      "tailwindcss": "^4.0.17",
      "typescript": "^5.8.2",
      "typescript-eslint": "^8.28.0"
   },
   "ct3aMetadata": {
      "initVersion": "7.36.2"
   },
   "overrides": {
      "uri-js": "npm:uri-js-replace"
   }
}
